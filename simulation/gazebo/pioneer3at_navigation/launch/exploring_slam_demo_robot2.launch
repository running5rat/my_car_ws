
<launch>
    

    <!--include file="$(find pioneer3at_navigation)/launch/gmapping_robot1.launch"/-->   
    
    <!-- 运行虚拟定位，兼容AMCL输出 -->
    <!--node pkg="fake_localization" type="fake_localization" name="fake_localization" output="screen" -->
    
    <!-- 对于虚拟定位，需要设置一个/odom与/map之间的静态坐标变换 -->
    <!--node pkg="tf" type="static_transform_publisher" name="map_odom_broadcaster" args="0 0 0 0 0 0 /map /robot1/odom 100" -->

    <include file="$(find pioneer3at_navigation)/launch/robot2_map.launch"/>  


    <!-- 运行move_base节点 -->
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find pioneer3at_navigation)/config/robot2/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find pioneer3at_navigation)/config/robot2/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find pioneer3at_navigation)/config/robot2/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find pioneer3at_navigation)/config/robot2/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find pioneer3at_navigation)/config/robot2/base_local_planner_params.yaml" command="load" />
  </node>

    <!-- 运行rviz -->
    <!--node pkg="rviz" type="rviz" name="rviz" args="-d $(find pioneer3at_navigation)/rviz/nav.rviz"/-->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find pioneer3at_navigation)/config/robot2/robot2.rviz"/>

</launch>
